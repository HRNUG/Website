<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hampton Roads .NET User Group (HRNUG)</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <nav class="navbar">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/" title="HRNUG"><img src="~/Images/HRNUG-Logo.png" alt="HRNUG Logo" /> HRNUG</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="#events">Events</a></li>                    
                    <li><a href="#info">About</a></li>
                    <li><a href="#speakers">Speakers</a></li>
                    <li><a href="#sponsors">Sponsors</a></li>
                    <li><a href="http://www.meetup.com/Hampton-Roads-NET-Users-Group/messages/boards/" target="_blank" title="Forums">Forums</a></li>
                    <li><a href="http://www.meetup.com/Hampton-Roads-NET-Users-Group/messages/boards/thread/48183712" target="_blank" title="Jobs">Jobs</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="https://github.com/hrnug" target="_blank" title="GitHub" class="hrnug-nav-image-a"><img src="~/Images/GitHub-Mark-32px.png" alt="GitHub Logo" /></a></li>
                    <li><a href="https://twitter.com/hrnug" target="_blank" title="Twitter" class="hrnug-nav-image-a"><img src="~/Images/Twitter-Logo-Blue.png" alt="Twitter Logo" /></a></li>
                    <li><a href="http://www.meetup.com/Hampton-Roads-NET-Users-Group/" target="_blank" title="Meetup" class="hrnug-nav-image-a"><img src="~/Images/Meetup-Logo.png" alt="Meetup Logo"/></a></li>
                    <li><a href="https://www.linkedin.com/grp/home?gid=1779534" target="_blank" title="LinkedIn" class="hrnug-nav-image-a"><img src="~/Images/LinkedIn-Logo.png" alt="LinkedIn Logo" /></a></li>
                    <li><a href="http://757dev.org/" target="_blank" title="Slack" class="hrnug-nav-image-a"><img src="~/Images/Slack-Logo.png" alt="Slack Logo" /></a></li>
                </ul>
            </div>
        </div>
    </nav>

    @RenderBody()

    <footer>
        <div class="container body-content">
            <hr />
            <p class="text-center">&copy; @DateTime.Now.Year - Hampton Roads .NET Users Group</p>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
